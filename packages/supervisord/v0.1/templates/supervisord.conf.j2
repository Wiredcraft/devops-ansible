[supervisord]
nodaemon = {% if supervisord.nodaemon is defined %}{{ supervisord.nodaemon }}{% else %}false{% endif %}


{% set inet = supervisord.inet | default({}) %}
{% if inet.port is defined and inet.port %}
[inet_http_server]
port = {{ inet.port }}
{% if inet.username is defined and inet.username %}
username = {{ inet.username }}
{% endif %}
{% if inet.password is defined and inet.password%}
password = {{ inet.password }}
{% endif %}
{% endif %}