COUCHDB_USER={{ couchdb.running_user }}
COUCHDB_STDOUT_FILE={{ couchdb.stdout_file }}
COUCHDB_STDERR_FILE={{ couchdb.stderr_file }}
COUCHDB_RESPAWN_TIMEOUT=5
COUCHDB_OPTIONS=

# Set this option to 0 if you don't want CouchDB server to start
# during system boot.
ENABLE_SERVER={{ couchdb.enable }}